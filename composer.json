{
    "name": "patpat/laravel-log-viewer",
    "description": "根据rap2hpoutre修改，增加了自定义log路径和读取子目录日志, thanks for rap2hpoutre",
    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "laravel", "lumen"],
    "authors": [
        {
            "name": "bruce",
            "email": "bruce.he@patpat.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*|5.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Rap2hpoutre\\LaravelLogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rap2hpoutre\\LaravelLogViewer\\LaravelLogViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
